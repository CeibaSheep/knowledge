                                                <script language="4">
                                                    <content>
                                                        <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        pass

    def onLoad(self):
        self.memory = ALProxy("ALMemory")

    def onUnload(self):
        self.memory = None

    def onInput_onStart(self,p):
        spliter = "{@@@}"
        goMethodName = ""
        #self.logger.warning(p)

        self.logger.info("type::"+str(isinstance(p, list)))

        if isinstance(p, list):
            command = []
            command.append(p)
        else:
            command = p.split(spliter)

        for c in command :
            if isinstance(c, list):
                com = c
            else:
                self.logger.warning("***"+c+"***")
                c = c.replace("'","")
                com = eval(c)

            if com[0] == "sendPepperSay":
                self.onSayText(com[2])
            if com[0] == "sendPepperVoice":
                self.memory.insertData("forceInputText",com[2])
                self.onForceInput(com[2])
            if com[0] == "sendPepperMemory":
                self.memory.insertData(com[1],com[2])
                self.logger.info("insertMemory::" + com[1]+ "::" +com[2])

            if com[0] == "sendPepperEvent":
                self.memory.raiseEvent(com[1],com[2])
                self.logger.info("raiseEvent::" + com[1]+ "::" +com[2])

            if com[0] == "sendPepperGoMethod":
                goMethodName = com[2]

        if goMethodName == "":
            self.onStopped("")
        else:
            self.onRaiseMethod(goMethodName)

    def onInput_onStop(self):
        self.onUnload()

        pass]]>
</content>
                                                </script>
