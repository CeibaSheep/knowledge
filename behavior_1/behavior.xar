    def __init__(self):
        GeneratedClass.__init__(self)
        pass

    def onLoad(self):
        self.memory = ALProxy("ALMemory")

    def onUnload(self):
        self.memory = None

    def onInput_onStart(self):
        dialog = ALProxy("ALDialog")
        knowledge = ALProxy("ALKnowledge")

        fishKnow = knowledge.getSubject("foods", "variation", "魚")
        if len(fishKnow) == 0:
            fishKnow.append("さば")

        meatKnow = knowledge.getSubject("foods", "variation", "肉")
        if len(meatKnow) == 0:
            meatKnow.append("牛肉")

        vegetableKnow = knowledge.getSubject("foods", "variation", "野菜")
        if len(vegetableKnow) == 0:
            vegetableKnow.append("にんじん")

        fruitsKnow = knowledge.getSubject("foods", "variation", "果物")
        if len(fruitsKnow) == 0:
            fruitsKnow.append("みかん")

        serialKnow = knowledge.getSubject("foods", "variation", "穀物")
        if len(serialKnow) == 0:
            serialKnow.append("お米")

        notCategorizeKnow = knowledge.getSubject("foods", "variation", "未分類")
        if len(notCategorizeKnow) == 0:
            notCategorizeKnow.append("ニョッキ")

        #self.logger.info(str(fishKnow))
        #self.logger.info(str(meatKnow))
        #self.logger.info(str(vegetableKnow))
        #self.logger.info(str(fruitsKnow))
        #self.logger.info(str(serialKnow))
        #self.logger.info(str(notCategorizeKnow))

        dialog.setConcept("fishKnow","jpj", fishKnow)
        dialog.setConcept("meatKnow","jpj", meatKnow)
        dialog.setConcept("vegetableKnow","jpj", vegetableKnow)
        dialog.setConcept("fruitsKnow","jpj", fruitsKnow)
        dialog.setConcept("serialKnow","jpj", serialKnow)
        dialog.setConcept("notCategorizeKnow","jpj", notCategorizeKnow)


        frtHalfJanKnow = knowledge.getSubject("foods", "season", "01月上旬")
        if len(frtHalfJanKnow) == 0:
            frtHalfJanKnow.append("ふきのとう")
        dialog.setConcept("frtHalfJanKnow","jpj", frtHalfJanKnow)

        sndHalfJanKnow = knowledge.getSubject("foods", "season", "01月下旬")
        if len(sndHalfJanKnow) == 0:
            sndHalfJanKnow.append("菜の花")
        dialog.setConcept("sndHalfJanKnow","jpj", sndHalfJanKnow)

        frtHalfFebKnow = knowledge.getSubject("foods", "season", "02月上旬")
        if len(frtHalfFebKnow) == 0:
            frtHalfFebKnow.append("いちご")
        dialog.setConcept("frtHalfFebKnow","jpj", frtHalfFebKnow)

        sndHalfFebKnow = knowledge.getSubject("foods", "season", "02月下旬")
